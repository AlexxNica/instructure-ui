{
  "name": "instructure-ui",
  "version": "1.0.1-beta",
  "description": "A React UI component library made by Instructure Inc.",
  "author": "Instructure, Inc. Engineering and Product Design",
  "main": "./lib/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/instructure/instructure-ui.git"
  },
  "homepage": "https://github.com/instructure/instructure-ui",
  "bugs": "https://github.com/instructure/instructure-ui/issues",
  "scripts": {
    "prestart": "npm run clean && npm install && npm run docs:build",
    "start": "http-server __build__/docs -p 8001",
    "start:watch": "webpack-dev-server --hot --inline --config webpack/docs.config.js",
    "test": "cross-env NODE_ENV=test karma start --single-run --coverage --ci",
    "test:lint": "eslint lib",
    "test:watch": "cross-env NODE_ENV=test karma start",
    "generate": "./scripts/generate",
    "clean": "rimraf __build__",
    "docs": "webpack --config webpack/docs.config.js",
    "docs:build": "cross-env NODE_ENV=production webpack --config webpack/docs.config.js",
    "dist": "cross-env NODE_ENV=production webpack --config webpack/dist.config.js",
    "dist:min": "cross-env MINIFY=1 NODE_ENV=production webpack --config webpack/dist.config.js",
    "transpile": "cross-env NODE_ENV=transpile BABEL_DISABLE_CACHE=1 babel lib --out-dir __build__/lib --ignore *.test.js",
    "build": "npm run clean && npm run dist && npm run dist:min && npm run transpile",
    "bump": "./scripts/bump",
    "bump:major": "./scripts/bump major",
    "bump:minor": "./scripts/bump minor",
    "bump:patch": "./scripts/bump patch",
    "prerelease": "npm install && npm test && npm run build",
    "release": "./scripts/release",
    "release:beta": "./scripts/release -t beta",
    "release:rc": "./scripts/release -t rc",
    "deploy": "npm run docs:build && gh-pages -d __build__/docs -r git@github.com:instructure/instructure-ui.git"
  },
  "license": "MIT",
  "devDependencies": {
    "autoprefixer": "~6.4.0",
